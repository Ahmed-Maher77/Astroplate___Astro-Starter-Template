---
import config from "@/config/config.json";
import {
IoBriefcase,
  IoLogoFacebook,
  IoLogoGithub,
  IoLogoLinkedin,
} from "react-icons/io5";

const { base_url }: { base_url: string } = config.site;
const {
  title,
  description,
  slug,
  className,
}: { title?: string; description?: string; slug?: string; className?: string } =
  Astro.props;
---

<ul class={`${className}`}>
  <li class="inline-block">
    <a
      aria-label="facebook share button"
      href={`https://web.facebook.com/profile.php?id=100012154268952`}
      target="_blank"
      rel="noreferrer noopener"
    >
      <IoLogoFacebook />
    </a>
  </li>
  <li class="inline-block">
    <a
      aria-label="portfolio link"
      href={`https://ahmedmaher-portfolio.vercel.app/`}
      target="_blank"
      rel="noreferrer noopener"
    >
    <IoBriefcase />
    </a>
  </li>
  <li class="inline-block">
    <a
      aria-label="linkedin share button"
      href={`https://www.linkedin.com/in/ahmed-maher-algohary`}
      target="_blank"
      rel="noreferrer noopener"
    >
      <IoLogoLinkedin />
    </a>
  </li>
  <li class="inline-block">
    <a
      aria-label="GitHub profile"
      href="https://github.com/Ahmed-Maher77"
      target="_blank"
      rel="noreferrer noopener"
    >
    <IoLogoGithub />
    </a>
  </li>
</ul>
